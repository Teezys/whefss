<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context.xsd">
	<bean id="callService" class="com.yuangou.ecp.bp.core.framework.handler.YGCallServiceHandler" scope="prototype"></bean>
	<bean id="addMsgHeader" class="com.yuangou.ecp.bp.core.framework.handler.YGAddHeaderHandler" scope="prototype"></bean>
	<bean id="callProcess" class="com.yuangou.ecp.bp.core.framework.handler.YGCallProcessHandler" scope="prototype"></bean>
	<bean id="monitor" class="com.yuangou.ecp.bp.core.framework.handler.YGMonitorHandler" scope="prototype"></bean>
	<bean id="putMsg" class="com.yuangou.ecp.bp.core.framework.handler.YGPutMsgHandler" scope="prototype"></bean>
	<bean id="waitMsg" class="com.yuangou.ecp.bp.core.framework.handler.YGWaitMsgHandler" scope="prototype"></bean>
</beans>
