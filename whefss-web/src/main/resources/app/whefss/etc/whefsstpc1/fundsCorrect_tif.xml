<?xml version='1.0' encoding='UTF-8'?>
<root>
    <format code="fundsCorrect" desc="查询冲正数据">
        <request necessary="yes">
            <xmlWrite name="Root">
                <quote name="request"/>
            </xmlWrite>
        </request>
        <response necessary="no">
            <xmlRead name="Root">
                <quote name="response"></quote>
                <xmlRead name="data">
                    <xmlRead name="notice_num" data_name="notice_num" desc="冲正申请书编号" necessary="no"/>
                    <xmlRead name="contract_num" data_name="contract_num" desc="购房合同编号" necessary="no"/>
                    <xmlRead name="bank_name" data_name="bank_name" desc="监管账户开户总行" necessary="no"/>
                    <xmlRead name="branch_name" data_name="branch_name" desc="监管账户开户支行" necessary="no"/>
                    <xmlRead name="account_name" data_name="account_name" desc="监管账户名称" necessary="no"/>
                    <xmlRead name="account_num" data_name="account_num" desc="监管账户账号" necessary="no"/>
                    <xmlRead name="amount" data_name="amount" desc="冲正金额" necessary="no"/>
                    <xmlRead name="sn" data_name="sn" desc="资金流水号" necessary="no"/>
                    <xmlRead name="time" data_name="time" desc="进帐时间" necessary="no"/>
                </xmlRead>
            </xmlRead>
        </response>
    </format>
</root>