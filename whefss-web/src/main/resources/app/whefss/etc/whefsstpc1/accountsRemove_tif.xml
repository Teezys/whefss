<?xml version='1.0' encoding='UTF-8'?>
<root>
    <format code="accountsRemove" desc="查询销户数据">
        <request necessary="yes">
            <xmlWrite name="Root">
                <quote name="request"/>
            </xmlWrite>
        </request>
        <response necessary="no">
            <xmlRead name="Root">
                <quote name="response"></quote>
                <xmlRead name="data">
                    <xmlRead name="notice_num" data_name="notice_num" desc="销户通知书编号" necessary="no"/>
                    <xmlRead name="monitor_num" data_name="monitor_num" desc="监管编号" necessary="no"/>
                    <xmlRead name="bank_name" data_name="bank_name" desc="监管账户开户总行" necessary="no"/>
                    <xmlRead name="branch_name" data_name="branch_name" desc="监管账户支行名称" necessary="no"/>
                    <xmlRead name="account_num" data_name="account_num" desc="监管账户账号" necessary="no"/>
                </xmlRead>
            </xmlRead>
        </response>
    </format>
</root>